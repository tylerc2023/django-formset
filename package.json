{
  "name": "django-formset",
  "version": "0.0.1",
  "description": "Django-Formset",
  "scripts": {
    "build": "node client/build.cjs",
    "build:watch": "node client/build.cjs --watch",
    "minify": "jsmin -o formset/static/formset/js/django-formset.min.js formset/static/formset/js/django-formset.js",
    "pegjs": "pegjs -o client/django-formset/actions.ts --extra-options-file pegjsconf.json --plugin ./node_modules/ts-pegjs/src/tspegjs.js assets/actions.pegjs",
    "tailwindcss": "tailwindcss build assets/tailwind-styles.css -o formset/static/formset/css/tailwind.css",
    "uglify": "uglifyjs formset/static/formset/js/django-formset.js --compress --mangle -o formset/static/formset/js/django-formset.min.js"
  },
  "license": "MIT",
  "devDependencies": {
    "@tailwindcss/forms": "^0.2.1",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.template": "^4.5.0",
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "autoprefixer": "^10.2.5",
    "bootstrap": "^4.6.0",
    "bulma": "^0.9.2",
    "esbuild": "^0.11.17",
    "esbuild-plugin-inline-import": "^1.0.1",
    "esbuild-plugin-sass": "^0.5.0",
    "foundation-sites": "^6.6.3",
    "lodash.get": "^4.4.2",
    "lodash.template": "^4.5.0",
    "playwright": "^1.10.0",
    "postcss": "^8.2.8",
    "sass": "^1.32.12",
    "tailwindcss": "^2.0.4",
    "ts-pegjs": "^0.3.1",
    "tslib": "^2.2.0",
    "typescript": "^4.1.4",
    "uglify-js": "^3.13.8"
  },
  "dependencies": {
    "tom-select": "^1.7.5"
  }
}
