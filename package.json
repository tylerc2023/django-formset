{
  "name": "django-formset",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "node client/build.cjs",
    "minify": "jsmin -o formset/static/formset/js/django-formset.min.js formset/static/formset/js/django-formset.js",
    "tag-attributes": "peggy -o client/django-formset/tag-attributes.ts --extra-options-file assets/pegjsconf.json --plugin ./node_modules/ts-pegjs/src/tspegjs.js assets/tag-attributes.pegjs",
    "tailwindcss": "tailwindcss --input assets/tailwind-styles.css --config assets/tailwind.config.js --output formset/static/formset/css/tailwind.css",
    "uglify": "uglifyjs formset/static/formset/js/django-formset.js --compress --mangle -o formset/static/formset/js/django-formset.min.js"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.0",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.set": "^4.3.6",
    "@types/lodash.template": "^4.5.0",
    "@types/lodash.zip": "^4.2.6",
    "@types/sortablejs": "^1.10.7",
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "autoprefixer": "^10.2.5",
    "bootstrap": "^5.1.1",
    "bulma": "^0.9.2",
    "esbuild": "^0.11.23",
    "esbuild-plugin-inline-import": "^1.0.1",
    "esbuild-plugin-sass": "^0.5.0",
    "foundation-sites": "^6.6.3",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "lodash.template": "^4.5.0",
    "lodash.zip": "^4.2.0",
    "peggy": "^1.2.0",
    "playwright": "^1.18.0",
    "postcss": "^8.4.5",
    "sass": "^1.32.12",
    "sortablejs": "^1.15.0",
    "tailwindcss": "^3.0.23",
    "tom-select": "^2.0.0",
    "ts-pegjs": "^1.2.1",
    "tslib": "^2.2.0",
    "typescript": "^4.1.4",
    "uglify-js": "^3.13.8",
    "uikit": "^3.7.5"
  }
}
