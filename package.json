{
  "name": "django-formset",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "scripts": {
    "build": "stencil build --docs --prod",
    "start": "stencil build --dev --watch --serve",
    "watch": "stencil build --dev --watch",
    "test.e2e": "stencil test --e2e --debug --dev --log --watch",
    "generate": "stencil generate",
    "tailwindcss": "tailwindcss build assets/tailwind-styles.css -o testapp/static/testapp/tailwind.css",
    "pegjs": "tspegjs -o client/components/django-formset/actions.ts --custom-header '// @ts-nocheck' assets/actions.pegjs"
  },
  "dependencies": {
    "@stencil/core": "^2.0.1",
    "@types/lodash": "^4.14.168",
    "bootstrap": "^4.6.0",
    "foundation-sites": "^6.6.3",
    "lodash.get": "^4.4.2"
  },
  "license": "MIT",
  "collection": "./formset/static/collection/collection-manifest.json",
  "main": "./formset/static/index.cjs.js",
  "module": "./formset/static/index.js",
  "types": "./formset/static/types/components.d.ts",
  "devDependencies": {
    "@stencil/sass": "^1.4.1",
    "@tailwindcss/forms": "^0.2.1",
    "autoprefixer": "^10.2.5",
    "bulma": "^0.9.2",
    "postcss": "^8.2.8",
    "tailwindcss": "^2.0.4",
    "ts-pegjs": "^0.3.1",
    "typescript": "^4.1.4"
  }
}
